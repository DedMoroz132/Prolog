sor([],[]).
sor([A|B],[]):-
    sor(B,A).
sor([A|B],C):-
    in(A,C,C1,0),
    sor(B,C1).
in(A,[],A,0).
in(A,[B|C],[D|E],0):-
    A>B,
    D is B,
    in(A,C,E,0).
in(A,[B|C],[D,E|F],0):-
    A=<B,
    D is A,
    E is B,
    in(A,C,F,1).
in(_,[],[],1).
in(A,[B|C],[D|E],1):-
    D is B,
    in(A,C,E,1).
